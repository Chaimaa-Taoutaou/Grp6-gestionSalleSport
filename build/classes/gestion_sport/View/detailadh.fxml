      <Button fx:id="imprimer" layoutX="303.0" layoutY="70.0" mnemonicParsing="false" onAction="#btnimprimer" prefHeight="28.0" prefWidth="125.0" style="-fx-background-color: #f5053d; -fx-border-radius: #cc0000;" text="Imprimer" textFill="WHITE">
